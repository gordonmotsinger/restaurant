<h1>Sessions#new</h1>
<p>Find me in app/views/sessions/new.html.erb</p>

<%= form_for :sessions, url: login_path do |f| %>
    <% if !@user.errors.empty? %>
        <% @user.errors.full_message.each do |error| %>
            <li><%= error %></li>
        <% end %>
    <% end %>
    <p>Name <%= f.text_field :login_name %>
    <p>Password <%= f.text_field :password %>

    <p><%= f.submit%>
<%end%>